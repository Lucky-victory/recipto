{
  "name": "recipto",
  "private": true,
  "version": "1.0.0",
  "description": "Recipto",
  "repository": "",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development vite",
    "postinstall": "cpy --flat ./node_modules/material-symbols/*.{woff2,scss,css} ./src/fonts/",
    "build": "cross-env NODE_ENV=production vite build && npx workbox generateSW workbox-config.js",
    "build-capacitor-android": "cross-env NODE_ENV=production vite build && npx cap copy android"
  },
  "browserslist": [
    "IOS >= 15",
    "Safari >= 15",
    "last 5 Chrome versions",
    "last 5 Firefox versions"
  ],
  "dependencies": {
    "@capacitor/android": "^5.0.4",
    "@capacitor/app": "^5.0.2",
    "@capacitor/browser": "^5.0.2",
    "@capacitor/core": "^5.0.4",
    "@capacitor/keyboard": "^5.0.2",
    "@capacitor/splash-screen": "^5.0.2",
    "@capacitor/status-bar": "^5.0.2",
    "dom7": "^4.0.6",
    "framework7": "^8.0.5",
    "framework7-react": "^8.0.5",
    "material-symbols": "^0.5.5",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "skeleton-elements": "^4.0.1",
    "swiper": "^9.3.2"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.0.4",
    "@vitejs/plugin-react": "^4.0.0",
    "cordova-res": "^0.15.4",
    "cpy-cli": "^4.2.0",
    "cross-env": "^7.0.3",
    "postcss-preset-env": "^8.4.1",
    "sass": "^1.62.1",
    "vite": "^4.3.8",
    "workbox-cli": "^6.5.4"
  }
}
